{
  "name": "js-simple-events",
  "version": "1.0.7",
  "description": "Yet another simple event management system",
  "main": "cjs/index.js",
  "module": "es5/index.js",
  "types": "types",
  "scripts": {
    "build-cjs": "tsc --p ./tsconfig.cjs.json",
    "build-es5": "tsc --p ./tsconfig.es5.json",
    "build": "npm run build-cjs && npm run build-es5",
    "postversion": "npm run build",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaskar2008/js-simple-events.git"
  },
  "keywords": [
    "Simple",
    "Events",
    "JS",
    "TS",
    "Light",
    "Management"
  ],
  "author": "kaskar2008",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaskar2008/js-simple-events/issues"
  },
  "homepage": "https://github.com/kaskar2008/js-simple-events#readme",
  "devDependencies": {
    "typescript": "^2.9.2"
  }
}
